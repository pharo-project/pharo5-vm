{
    "package": {
        "name": "build",
        "repo": "pharo-vm",
		"subject": "pharo-project"
    },
    "version": {
        "name": "$Rev$",
        "desc": "Automatic build"
    },
    "files": [
        {
				"includePattern": "./(pharo-.*\\.(?:gz|zip))", 
				"uploadPattern": "$1", 
				"matrixParams": { "override": 1 }
		}
    ],
    "publish": true
}
